{
  "name": "moccasin-store",
  "version": "1.0.0",
  "description": "An e-commerce shoe store",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "node src/index.js",
    "migrations": "npx sequelize-cli db:migrate",
    "undo:migrations": "npx sequelize-cli db:migrate:undo:all",
    "seed": "npx sequelize-cli db:seed:all",
    "undo:seed": "npx sequelize-cli db:seed:undo:all",
    "test": "SET \"NODE_ENV=test\" && mocha src/test/**/*.test.js --timeout=15000",
    "test-watch": "nodemon --exec \"npm test\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anjorlar/moccasinStore.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/anjorlar/moccasinStore/issues"
  },
  "homepage": "https://github.com/anjorlar/moccasinStore#readme",
  "devDependencies": {
    "expect": "^28.0.2",
    "mocha": "^9.2.2",
    "nodemon": "^2.0.16",
    "sequelize-cli": "^6.4.1",
    "supertest": "^6.2.3"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.0",
    "helmet": "^5.0.2",
    "http-status-codes": "^2.2.0",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "sequelize": "^6.19.0",
    "winston": "^3.7.2",
    "winston-daily-rotate-file": "^4.6.1"
  }
}
